<actions>
<action>
	<icon>utilities-terminal</icon>
	<name>Open Terminal Here</name>
	<submenu></submenu>
	<unique-id>1613914055840245-1</unique-id>
	<command>sh -c &quot;cd %F; ~/.scripts/terminal.sh&quot;</command>
	<description>Open Terminal Emulator from current directory</description>
	<range></range>
	<patterns>*</patterns>
	<startup-notify/>
	<directories/>
